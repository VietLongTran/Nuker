{
  "name": "Nuker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created to nuke a tweet. Boom boom.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}